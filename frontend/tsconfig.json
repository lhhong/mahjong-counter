{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "allowJs": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "noEmit": true,
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "jsx": "react"
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "../commons"
    }
  ]
}
